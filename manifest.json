{
    "name": "Digital Notice Board",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "diginotice.html",
        "default_icon": "logo.png"
    },
    "icons": {
        "128": "logo.png"
    },
    "permissions": [
        "storage",
        "alarms",
        "notifications"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "options_page": "options.html"
}